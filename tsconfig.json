{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@*": [
        "./*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@interfaces/*": [
        "interfaces/*"
      ],
      "@services/*": [
        "services/*"
      ],
